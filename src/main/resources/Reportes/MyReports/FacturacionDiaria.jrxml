	<parameter name="fecha" class="java.lang.String"/>
	<parameter name="formatoFecha" class="java.lang.String"/>
	<queryString language="SQL">
		<![CDATA[SELECT 
	C.TAMANIO TAMANIO_CARGA, C.PESO PESO_CARGA, R.ORIGEN ORIGEN_RUTA, R.DESTINO DESTINO_RUTA, PD.DESCRIPCION PUERTO_DESCARGA
FROM 
	CARGA C 
JOIN 
	RUTA R 
ON(R.IDRUTA=C.RUTA_IDRUTA)
JOIN
	ITINERARIO I
ON(I.RUTA_IDRUTA=R.IDRUTA)
JOIN
	ETAPA E
ON(E.ITINERARIO_IDITINERARIO=I.IDITINERARIO)
JOIN
	VIAJES V
ON(V.ETAPA_IDETAPA=E.IDETAPA)
JOIN
	PUERTOS PD
ON(PD.IDPUERTOS=V.PUERTOLLEGADA)
WHERE C.ESTADO='DESCARGADO' AND V.FECHALLEGADA=STR_TO_DATE($P{fecha}, $P{formatoFecha})]]>
	</queryString>
